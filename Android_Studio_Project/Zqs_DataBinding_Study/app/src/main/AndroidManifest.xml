<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.zoran.zqs_databinding_study">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".basic.BasicActivity"/>
        <activity android:name=".custombinding.CustomBindingActivity"/>
        <activity android:name=".include.IncludeActivity"/>
        <activity android:name=".resource.ResourceActivity"/>
        <activity android:name=".collection.CollectionActivity"/>
        <activity android:name=".observable.ObservableActivity"/>
        <activity android:name=".viewwithids.ViewWithIDsActivity"/>
        <activity android:name=".viewstub.ViewStubActivity"/>
        <activity android:name=".dynamic.DynamicActivity"/>
        <activity android:name=".conversions.ConversionsActivity"/>
    </application>

</manifest>